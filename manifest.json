{
    "manifest_version": 3,
    "name": "Lockout Limit",
    "description": "WARNING ! Your excuses won't be tolerated.",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
      },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
       "tabs",
       "activeTab",
       "storage"
    ]
  }
